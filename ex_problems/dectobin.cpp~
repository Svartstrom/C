#include <iostream>
#include <cmath>

using namespace std;

int maxPow(int n)
{
  int i = 1;
  while (pow(2,i) <= n)
    {
      i++;
    }
  return i;
}



int main(int argc, char *argv[])
{
  if ( argc < 2 || argc > 2 )
  {
    cout<<"Usage: dectobin ddd"<<endl;
    return 1;
  }

  n = int(argv[1]);
  cout<<n;
}
